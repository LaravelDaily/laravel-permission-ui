{
    "name": "laraveldaily/permissions-ui",
    "description": "A Laravel package for adding UI for spatie laravel permissions package.",
    "type": "library",
    "homepage": "https://github.com/LaravelDaily/laravel-permissions-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Povilas Korop",
            "email": "povilas@laraveldaily.com"
        }
    ],
    "keywords": [
        "spatie",
        "laravel-permissions",
        "laraveldaily",
        "laravel",
        "laravel-package"
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelDaily\\PermissionsUI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelDaily\\PermissionsUI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDaily\\PermissionsUI\\PermissionsUIServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
